com.br\/wp-content\/uploads\/2026\/01\/Frame-1321317914-1-768x738.png 768w\" sizes=\"(max-width: 800px) 100vw, 800px\" \/>\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<div class=\"elementor-element elementor-element-ff7dffe elementor-widget elementor-widget-html\" data-id=\"ff7dffe\" data-element_type=\"widget\" data-widget_type=\"html.default\">\n\t\t\t\t\t<script>\ndocument.addEventListener(\"DOMContentLoaded\", function () {\n  const paicard = document.querySelector(\".paicard\");\n  if (!paicard) return;\n\n  const cards = paicard.querySelectorAll(\".cards > .e-child\");\n\n  cards.forEach(card => {\n    card.addEventListener(\"click\", function () {\n      \/\/ acha a classe do tipo card1, card2 ou card3\n      const baseCard = Array.from(card.classList).find(c => \/^card[1-3]$\/.test(c));\n      if (!baseCard) return;\n\n      const num = baseCard.replace(\"card\", \"\"); \/\/ \"1\", \"2\" ou \"3\"\n\n      \/\/ seleciona o conte\u00fado correspondente\n      const conteudoNovo = paicard.querySelector(\".card-conteudo .conteudo\" + num);\n      if (!conteudoNovo) {\n        console.warn(\"N\u00e3o encontrei .conteudo\" + num);\n        return;\n      }\n\n      \/\/ se j\u00e1 t\u00e1 ativo, n\u00e3o faz nada\n      if (conteudoNovo.classList.contains(\"conteudo\" + num + \"-ativo\")) {\n        return;\n      }\n\n      \/\/ remove ativos dos cards\n      paicard.querySelectorAll(\".card1-ativo, .card2-ativo, .card3-ativo\")\n        .forEach(el => el.classList.remove(\"card1-ativo\", \"card2-ativo\", \"card3-ativo\"));\n\n      \/\/ remove ativos dos conte\u00fados\n      paicard.querySelectorAll(\".conteudo1-ativo, .conteudo2-ativo, .conteudo3-ativo\")\n        .forEach(el => el.classList.remove(\"conteudo1-ativo\", \"conteudo2-ativo\", \"conteudo3-ativo\"));\n\n      \/\/ ativa o card clicado\n      card.classList.add(\"card\" + num + \"-ativo\");\n\n      \/\/ ativa o conte\u00fado correspondente\n      conteudoNovo.classList.add(\"conteudo\" + num + \"-ativo\");\n    });\n  });\n});\n<\/script>\n\t\t\t\t<\/div>\n\t\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t<div class=\"elementor-element elementor-element-a5e482e e-flex e-con-boxed e-con e-parent\" data-id=\"a5e482e\" data-element_type=\"container\">\n\t\t\t\t\t<div class=\"e-con-inner\">\n\t\t<div class=\"elementor-element elementor-element-15d32d9 e-con-full e-flex e-con e-child\" data-id=\"15d32d9\" data-element_type=\"container\">\n\t\t\t\t<div class=\"elementor-element elementor-element-0de1693 elementor-widget__width-initial scroll-bottom elementor-widget elementor-widget-heading\" data-id=\"0de1693\" data-element_type=\"widget\" data-widget_type=\"heading.default\">\n\t\t\t\t\t<p class=\"elementor-heading-title elementor-size-default\">Embasada em \nquem <span style=\"font-weight:600\" class=\"deg\">vendeu milh\u00f5es na pr\u00e1tica<\/span><\/p>\t\t\t\t<\/div>\n\t\t\t\t<div class=\"elementor-element elementor-element-76172cf elementor-widget__width-initial scroll-bottom e1 elementor-widget elementor-widget-text-editor\" data-id=\"76172cf\" data-element_type=\"widget\" data-widget_type=\"text-editor.default\">\n\t\t\t\t\t\t\t\t\t<p>N\u00e3o s\u00e3o frases bonitas nem promessas vazias. A experi\u00eancia dentro do RiseCheckout \u00e9 constru\u00edda a partir do que realmente funciona no campo de batalha das vendas online: clareza, convers\u00e3o e decis\u00e3o.<\/p>\t\t\t\t\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<div class=\"elementor-element elementor-element-e69c0ed text scroll-bottom elementor-widget-mobile__width-initial elementor-widget elementor-widget-heading\" data-id=\"e69c0ed\" data-element_type=\"widget\" data-widget_type=\"heading.default\">\n\t\t\t\t\t<p class=\"elementor-heading-title elementor-size-default\"><span class=\"deg\">ALGUNS DOS CLIENTES QUE VALIDAM O RISECHECKOUT NA PR\u00c1TICA:<\/span><\/p>\t\t\t\t<\/div>\n\t\t\t\t<div class=\"elementor-element elementor-element-c0a1e6d carrossel scroll-bottom elem