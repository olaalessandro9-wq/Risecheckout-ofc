=> {\n      const p = self.progress;\n\n      const price1 = document.querySelector(\".container-price1\");\n      const price2 = document.querySelector(\".container-price2\");\n\n      if (p > 0.1) {\n        price1?.classList.add(\"container-price1-ativo\");\n      } else {\n        price1?.classList.remove(\"container-price1-ativo\");\n      }\n\n      if (p > 0.5) {\n        price2?.classList.add(\"container-price2-ativo\");\n      } else {\n        price2?.classList.remove(\"container-price2-ativo\");\n      }\n    }\n  });\n});\n<\/script>\n\n\n\n\n\n<script>\n    \n    \n        \n\n    \n    \ndocument.addEventListener(\"DOMContentLoaded\", function() {\n  console.log('JS carregado!');\n\n  const buttons = document.querySelectorAll('.bt .elementor-button');\n  const brilhoWidth = 15; \/\/ largura do brilho em %\n\n  buttons.forEach(button => {\n    let lastPercentage = 0;\n\n    button.addEventListener('mousemove', (e) => {\n      const rect = button.getBoundingClientRect();\n      const mouseX = e.clientX - rect.left;\n      const buttonWidth = rect.width;\n\n      let percentage = ((mouseX \/ buttonWidth) * 100) - (brilhoWidth \/ 2);\n\n      if (percentage < 0) percentage = 0;\n      if (percentage > 100 - brilhoWidth) percentage = 100 - brilhoWidth;\n\n      button.style.setProperty('--left', `${percentage}%`);\n      lastPercentage = percentage;\n    });\n\n    button.addEventListener('mouseleave', () => {\n      button.style.setProperty('--left', `${lastPercentage}%`);\n    });\n  });\n\n  if (!buttons.length) {\n    console.log('Nenhum bot\u00e3o encontrado!');\n  }\n});\n\n<\/script>\n\n\n\n\n\n\n\n\n\n\n<script>\n            \/*\nCODIGO DO texto\n*\/\n    \ndocument.addEventListener('DOMContentLoaded', function () {\n    const effects = document.querySelectorAll('.effect');\n    const img2 = document.querySelector('.img2');\n\n    if (!effects.length || !img2) return;\n\n    const words = ['criativos', 'conte\u00fados', 'p\u00e1ginas', 'legendas', 'posts', 'stories'];\n    let wordIndex = 0;\n\n    const holdTime = 2000;\n    const typeSpeed = 90;\n    const eraseSpeed = 60;\n\n    img2.classList.remove('img2-off');\n    img2.classList.add('img2-ativo');\n\n    effects.forEach(span => {\n        function erase(text, index) {\n            img2.classList.remove('img2-ativo');\n            img2.classList.add('img2-off');\n\n            if (index >= 0) {\n                span.textContent = text.slice(0, index);\n                setTimeout(() => erase(text, index - 1), eraseSpeed);\n            } else {\n                wordIndex = (wordIndex + 1) % words.length;\n                setTimeout(() => type(words[wordIndex], 0), 300);\n            }\n        }\n\n        function type(text, index) {\n            if (index <= text.length) {\n                span.textContent = text.slice(0, index);\n                setTimeout(() => type(text, index + 1), typeSpeed);\n            } else {\n                img2.classList.remove('img2-off');\n                img2.classList.add('img2-ativo');\n                setTimeout(() => erase(text, text.length), holdTime);\n            }\n        }\n\n        span.textContent = words[wordIndex];\n        setTimeout(() => erase(words[wordIndex], words[wordIndex].length), holdTime);\n    });\n});\n<\/script>\n\n\n\n\n\n\n\n\n<script>\n    \n    \n        \/*\nCODIGO DO carrossel\n*\/\n    \n    \n    \n    \n    \n    \ndocument.addEventListener('DOMContentLoaded', function () {\n    const carrossel = document.querySelector('.carrossel');\n    if (!carrossel) return;\n\n    const arrows = document.querySelectorAll(\n        '.elementor-swiper-button-next, .elementor-swiper-button-prev'\n    );\n\n    arrows.forEach(btn => {\n        btn.addEventListener('click', () => {\n\n            \/\/ 1\ufe0f\u20e3 tira o clip-path imediatamente\n            carrossel.classList.add('carrossel-ativo');\n\n            \/\/ 2\ufe0f\u20e3 espera 2 segundos para sumir o ::before\n            setTimeout(() => {\n                carrossel.classList.add('fade-before');\n            }, 2000); \/\/ 2000ms = 2s\n\n        });\n    });\n});\n<\/script>\n\n\n\n\n\n\n\n<script>\n    \n    \n        \n        \/*\nCODIGO DOa lista de icoen hover\n*\/\n\n\n\n    document.addEventListener(\"DOMContentLoaded\", () => {\n  document.querySelectorAll(\".lista-de-icones\").forEach((wrap) => {\n    const items = wrap.querySelectorAll(\".elementor-icon-list-item\");\n\n    items.forEach((item) => {\n      item.addEventListener(\"mouseenter\", () => {\n        wrap.classList.add(\"is-hovering-item\");\n        items.forEach(i => i.classList.remove(\"is-active\"));\n        item.classList.add(\"is-active\");\n      });\n\n      item.addEventListener(\"mouseleave\", () => {\n        item.classList.remove(\"is-active\");\n        wrap.classList.remove(\"is-hovering-item\");\n      });\n    });\n\n    wrap.addEventListener(\"mouseleave\", () => {\n      items.forEach(i => i.classList.remove(\"is-active\"));\n      wrap.classList.remove(\"is-hovering-item\");\n    });\n  });\n});\n\n<\/script>\n\n\n\n\n\n\n\n\n<script>\n  const elementos = document.querySelectorAll('.scroll-left, .scroll-right, .scroll-bottom, .scroll-top');\n  const posicaoScroll = window.innerHeight * 0.5;\n\n  function fadeScroll(){\n    elementos.forEach((el) => {\n      const elementoTop = el.getBoundingClientRect().top - posicaoScroll;\n      if (elementoTop < 320) {\n        el.classList.add('ativo');\n      } else {\n        el.classList.remove('ativo');\n      }\n    });\n  }\n\n  window.addEventListener('scroll', fadeScroll, { passive: true });\n  window.addEventListener('resize', fadeScroll);\n  document.addEventListener('DOMContentLoaded', fadeScroll);\n<\/script>\n\n\n\n\n\n<style>\n  @media only screen and (min-width: 600px) {\n.cursor-inner {\n    position: fixed;\n    width: 8px;\n    height: 8px;\n    border-radius: 50%;\n    background-color: white;\n    pointer-events: none;\n    z-index: 9999999999999999;\n    transform: translate(-50%, -50%);\n  }\n\n  .cursor-outer {\n    position: fixed;\n    width: 40px;\n    height: 40px;\n    border: 2px solid #6DCAF7;\n    border-radius: 50%;\n    pointer-events: none;\n    z-index: 9999999999999999;\n    transform: translate(-50%, -50%);\n  }}\n<\/style>\n\n<script>\n  document.body.insertAdjacentHTML('beforeend', '<div class=\"cursor-inner\"><\/div><div class=\"cursor-outer\"><\/div>');\n\n  const cursorInner = document.querySelector(\".cursor-inner\");\n  const cursorOuter = document.querySelector(\".cursor-outer\");\n\n  let mouseX = 0, mouseY = 0;\n  let innerX = 0, innerY = 0;\n  let outerX = 0, outerY = 0;\n\n  const lerp = (start, end, factor) => start + (end - start) * factor;\n\n  document.addEventListener(\"mousemove\", (e) => {\n    mouseX = e.clientX;\n    mouseY = e.clientY;\n  });\n\n  function animateCursor() {\n    innerX = lerp(innerX, mouseX, 0.25); \/\/ cursor interno: mais suave\n    innerY = lerp(innerY, mouseY, 0.25);\n\n    outerX = lerp(outerX, mouseX, 0.15); \/\/ cursor externo: mais \u00e1gil\n    outerY = lerp(outerY, mouseY, 0.15);\n\n    cursorInner.style.left = `${innerX}px`;\n    cursorInner.style.top = `${innerY}px`;\n\n    cursorOuter.style.left = `${outerX}px`;\n    cursorOuter.style.top = `${outerY}px`;\n\n    requestAnimationFrame(animateCursor);\n  }\n\n  animateCursor(); \/\/ iniciar a anima\u00e7\u00e3o\n<\/script>\n\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t","protected":false},"excerpt":{"rendered":"<p>A plataforma de checkout mais completa do Brasil Venda seus produtos digitais com\u00a0alta convers\u00e3o Transforme cada acesso em pagamento com um checkout personaliz\u00e1vel, r\u00e1pido e otimizado para escalar. Criar conta gr\u00e1tis agora Centenas de pessoas j\u00e1 est\u00e3o aplicando o passo a passo! Funcionalidades Tudo que voc\u00ea precisa para vender online, sem complica\u00e7\u00e3o O RiseCheckout foi [&hellip;]<\/p>\n","protected":false},"author":1,"featu